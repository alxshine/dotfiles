{
    "workbench.colorTheme": "Ayu Mirage",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "editor.fontFamily": "'Source Code Pro'",
    "editor.fontSize": 16,
    "files.simpleDialog.enable": true,
    "julia.focusPlotNavigator": true,
    "julia.symbolCacheDownload": true,
    "julia.additionalArgs": ["-t", "auto"],
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.fontSize": 15,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "compact",
    "RainbowBrackets.colorMode": "Independent",
    "julia.enableTelemetry": false,
    "explorer.confirmDelete": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.outDir": "%DIR%/_build",
    "git.confirmSync": false,
    "godot.csharp.executablePath": "/home/alex/.local/bin/godot",
    "[csharp]": {
        "editor.defaultFormatter": "csharpier.csharpier-vscode",
        "editor.formatOnSave": true
    },
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.wrapTabs": true,
    "terminal.external.linuxExec": "kitty",
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "redhat.telemetry.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.telemetry.enabled": false,
    "gitlens.hovers.enabled": false,
    "todo-tree.regex.regex": "(//|%|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
    "explorer.confirmDragAndDrop": false,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "latex-workshop.bind.altKeymap.enabled": true,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "svelte.enable-ts-plugin": true,
    "[svelte]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "debug.allowBreakpointsEverywhere": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.formatting.provider": "black",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "chat.experimental.quickQuestion.enable": true,
    "gitlens.graph.layout": "editor",
    "prettier.semi": false,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "julia.executablePath": "/home/alex/.local/opt/julia-1.8.5/bin/julia",
    "window.menuBarVisibility": "toggle",
    "editor.inlineSuggest.enabled": true,
    "git.enableSmartCommit": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "chat.experimental.quickQuestion.enable": true,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "cmake.configureOnOpen": false,
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inactiveRegionOpacity": 0.9,
}