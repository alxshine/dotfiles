{
    "workbench.colorTheme": "Gruvbox Minor Dark Medium",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "editor.fontFamily": "'Source Code Pro','Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "files.simpleDialog.enable": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "julia.focusPlotNavigator": true,
    "julia.symbolCacheDownload": true,
    "julia.additionalArgs": ["-t", "auto"],
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.sendKeybindingsToShell": true,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "compact",
    "RainbowBrackets.colorMode": "Independent",
    "julia.enableTelemetry": false,
    "RainbowBrackets.depreciation-notice": false,
    "explorer.confirmDelete": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.outDir": "%DIR%/_build",
}